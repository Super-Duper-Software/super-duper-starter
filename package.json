{
  "name": "super-duper-starter",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -- turbo run dev",
    "db": "dotenv -- pnpm --filter @superdupersoftware/db",
    "format-lint": "biome check .",
    "format-lint:fix": "biome check . --write",
    "turbo": "turbo",
    "typecheck": "turbo run typecheck",
    "prepare": "husky",
    "storybook": "pnpm --filter @superdupersoftware/ui storybook"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "husky": "^9.1.7",
    "turbo": "^2.7.5",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
